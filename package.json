{
  "name": "workers-loki-logger",
  "version": "0.1.2",
  "description": "A Logger for the Cloudflare workers environment that sends the logs to grafana loki",
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "files": [
    "lib/"
  ],
  "type": "module",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "test": "NODE_NO_WARNINGS=1 tap --node-arg=--experimental-specifier-resolution=node --node-arg=--loader=ts-node/esm",
    "prepublishOnly": "npm patch && npm run build"
  },
  "keywords": [
    "cloudflare",
    "logger",
    "grafana",
    "loki",
    "worker"
  ],
  "author": "Donato Wolfisberg (donato@wolfibserg.dev)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "17.0.25",
    "@types/tap": "15.0.6",
    "tap": "16.0.1",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
